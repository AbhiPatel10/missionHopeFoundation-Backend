{
  "version": 2,
  "env": {
    "MONGO_URI": "@MONGO_URI",
    "EMAIL_USER": "@EMAIL_USER",
    "EMAIL_PASS": "@EMAIL_PASS"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    }
  ]
}